@model IndexPageViewModel
@{
    ViewData["Title"] = "Admin Dashboard";
}
<h2>Admin Dashboard</h2>
<h3>Transactions</h3>
<table class="table">
    <thead>
        <tr>
            <th>User Email</th>
            <th>Amount</th>
            <th>Transaction Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var transaction in Model.Transactions)
        {
            <tr>
                <td>@transaction.UserEmail</td>
                <td>@transaction.Amount</td>
                <td>@transaction.TransactionDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>

<h3>Consultants</h3>
<ul>
    @foreach (var consultant in Model.Consultants)
    {
        <li>@consultant.Email - @consultant.LastName</li>
    }
</ul>

<h3>Financial Advisors</h3>
<ul>
    @foreach (var finAdvisor in Model.FinAdvisor)
    {
        <li>@finAdvisor.Email - @finAdvisor.LastName</li>
    }
</ul>
